{
  "name": "antd-table-scroll-xaxis-top",
  "version": "1.0.4",
  "main": "dist/index.umd.js",
  "description": "scroll table xaxis at top in ant design",
  "types": "type/index.d.ts",
  "typings": "type/index.d.ts",
  "module": "module/index.js",
  "esnext": "module/index.js",
  "jsnext:main": "module/index.js",
  "unpkg": "dist/index.umd.min.js",
  "jsdelivr": "dist/index.umd.min.js",
  "umd:main": "dist/index.umd.min.js",
  "license": "MIT",
  "author": "superwf@gmail.com",
  "sideEffects": true,
  "scripts": {
    "unit": "jest src/index.test.tsx",
    "e2e": "playwright test",
    "test": "yarn unit && yarn e2e",
    "start": "NODE_ENV=development OPEN=true webpack serve -c webpack.config.demo.ts",
    "buildDemo": "NODE_ENV=production webpack -c webpack.config.demo.ts",
    "analyze": "ANALYZE=true yarn buildDemo",
    "clean": "rimraf {module,dist,lib,type} -f",
    "badge": "jest-coverage-badges output ./badge",
    "buildDistDevelopment": "NODE_ENV=development rollup -c rollup.config.js",
    "buildDistProduction": "NODE_ENV=production rollup -c rollup.config.js",
    "build": "tsc -p tsconfig.module.json && tsc -p tsconfig.declaration.json && yarn buildDistDevelopment && yarn buildDistProduction && cp type/* module/ && cp type/* dist/",
    "copy-css": "cp src/*.css modlue/",
    "prepublishOnly": "yarn test && yarn build && standard-version",
    "prepare": "husky install && playwright install"
  },
  "files": [
    "module",
    "dist",
    "type",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/superwf/antd-table-scroll-xaxis-top/issues"
